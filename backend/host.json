{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Host.Results": "Warning",
      "Host.Aggregator": "Warning", 
      "Host.Function": "Warning",
      "Function": "Information"
    },
    "console": {
      "isEnabled": true
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "cors": {
    "credentials": false,
    "allowedOrigins": [
      "*"
    ]
  }
}